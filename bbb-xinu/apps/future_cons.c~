#include <xinu.h>
#include <future.h>

uint32 future_cons(future *fut) {
  printf("In consumer\n");
  int i, status;
  status = future_get(fut, &i);
printf("After coming from get \n %d", i);
  if (status < 1) {
    printf("future_get failed\n");
    return -1;
  }
  printf("it produced %d\n", i);
  future_free(fut); 
  return OK;
}
